	<SoundDef Abstract="True" Name="StomachSounds">
		<context>MapOnly</context>
		<sustain>False</sustain>
		<maxVoices>10</maxVoices>
		<maxSimultaneous>3</maxSimultaneous>
		<priorityMode>PrioritizeNearest</priorityMode>
	</SoundDef>

	<!-- Stomach Gurgle Sounds -->
	<SoundDef ParentName="StomachSounds">
		<defName>RR_StomachGurgles_Light</defName>
		<subSounds>
			<li>
				<muteWhenPaused>True</muteWhenPaused>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>Organic/Stomach/Gurgle/GurgleLight</clipFolderPath>
					</li>
				</grains>
				<volumeRange>70~70</volumeRange>
				<distRange>10~30</distRange>
				<sustainLoop>False</sustainLoop>
				<paramMappings>
					<li>
						<inParam Class="RimRound.Sound.SoundParamSource_StomachGurgles" />
						<outParam Class="SoundParamTarget_Volume" />
					</li>
				</paramMappings>
			</li>
		</subSounds>
	</SoundDef>
	
