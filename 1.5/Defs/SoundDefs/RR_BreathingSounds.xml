		<context>MapOnly</context>
		<sustain>True</sustain>
		<maxVoices>10</maxVoices>
		<maxSimultaneous>3</maxSimultaneous>
		<priorityMode>PrioritizeNearest</priorityMode>
	</SoundDef>

	<!-- Female Sounds -->
	<SoundDef ParentName="BreathingSounds">
		<defName>RR_F_BreathLight</defName>
		<subSounds>
			<li>
				<muteWhenPaused>True</muteWhenPaused>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>Organic/Breath/Regular/Breathing_F_Light</clipPath>
					</li>
				</grains>
				<volumeRange>40~50</volumeRange>
				<distRange>3~30</distRange>
				<paramMappings>
					<li>
						<inParam Class="RimRound.Sound.SoundParamSource_BreathingNormal" />
						<outParam Class="SoundParamTarget_Volume" />
					</li>
					<li>
						<inParam Class="RimRound.Sound.SoundParamSource_FemaleGlobal" />
						<outParam Class="SoundParamTarget_Volume" />
					</li>
				</paramMappings>
			</li>
